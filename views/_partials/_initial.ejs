</div>
<% } %>

<div id="snackbar"></div>

<div class="w3-container" style="margin-top:1%">


<button type="button" class="btn btn-outline-success" onclick="history.back()"><i class="bi bi-arrow-left"></i></button>
<br>
<br>

<div class="container-fluid">
<% if(info && info.length >0) { %>
<div class="alert alert-info" role="alert">
<div class="row">
<div class="col-auto w3-right">
<a href="#close" class="btn-close w3-right" data-bs-dismiss="alert" aria-label="Close"></a>
</div>
<div class="col-auto">
<div class="text-center">
<%= info %>
</div>
</div>
</div>
</div>
<% } %>
</div>

<div class="container-fluid">
<% if(error && error.length >0) { %>
<div class="alert alert-danger" role="alert">
<div class="row">
<div class="col-auto w3-right">
<a href="#close" class="btn-close w3-right" data-bs-dismiss="alert" aria-label="Close"></a>
</div>
<div class="col-auto">
<div class="text-center">
<%= error %>
</div>
</div>
</div>
</div>
<% } %>
